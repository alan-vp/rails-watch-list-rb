<div class="container mt-4">
  <%= simple_form_for @list do |f| %>
    <div class="mb-3">
      <%= f.input :name, label_html: {class: 'form-label'}, input_html: {class: 'form-control'} %>
    </div>
    <div class="mb-3">
      <%= f.input :photo, as: :file %>
    </div>
      <%= f.submit class: 'btn btn-primary mb-5' %>
  <% end %>
  <%= link_to "Back", lists_path %>
</div>
